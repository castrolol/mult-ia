{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["GOOGLE_GENERATIVE_AI_API_KEY","DATABASE_URL"]
    },
   "migrate": {
      "dependsOn": [
        "^migrate"
      ],
      "env": [
        "DATABASE_URL"
      ]
    },
    "grt": {
      "dependsOn": [
        "^grt"
      ],
      "outputs": [
        "../../packages/drizzle/entities/**"
      ],
      "env": [
        "DATABASE_URL",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET",
        "NEXT_PUBLIC_APP_URL"
      ]
    }
  }
}
